<Syncfusion:RibbonWindow x:Class="DocumentEditor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:Syncfusion="http://schemas.syncfusion.com/wpf"
        xmlns:System="clr-namespace:System;assembly=mscorlib"
        xmlns:this="clr-namespace:DocumentEditor"
        Icon="App.ico" Office2010Icon="/Assets/Syncfusion.png" SnapsToDevicePixels="True" IsGlassActive="False" 
        x:Name="ribbonWindow" Title="{Binding DocumentTitle}" Width="1087" Height="635"
        WindowState="Normal" WindowStartupLocation="CenterScreen" Syncfusion:SkinStorage.VisualStyle="Office2013">

    <Syncfusion:RibbonWindow.Resources>
        <Syncfusion:FontFamilyStringConverter x:Key="FontFamilyStringConverter"/>
        <Syncfusion:LeftAlignmentToggleConverter x:Key="LeftAlignmentToggleConverter"/>
        <Syncfusion:CenterAlignmentToggleConverter x:Key="CenterAlignmentToggleConverter"/>
        <Syncfusion:RightAlignmentToggleConverter x:Key="RightAlignmentToggleConverter"/>
        <Syncfusion:JustifyAlignmentToggleConverter x:Key="JustifyAlignmentToggleConverter"/>
        <Syncfusion:SingleStrikeThroughToggleConverter x:Key="SingleStrikeThroughToggleConverter"/>
        <Syncfusion:DoubleStrikeThroughToggleConverter x:Key="DoubleStrikeThroughToggleConverter"/>
        <Syncfusion:SubscriptToggleConverter x:Key="SubscriptToggleConverter"/>
        <Syncfusion:SuperscriptToggleConverter x:Key="SuperscriptToggleConverter"/>
        <Syncfusion:UnderlineToggleConverter x:Key="UnderlineToggleConverter"/>
        <Syncfusion:LineSpacingTypeStringConverter x:Key="LineSpacingTypeStringConverter"/>
        <Syncfusion:DoubleStringConverter x:Key="DoubleStringConverter"/>
        <Syncfusion:HighlightColorToggleConverter x:Key="HighlightColorToggleConverter"/>
        <this:DocumentEditorResourceWrapper x:Key="DocumentEditorResourceWrapper"/>
        <this:LineSpacingToggleConverter x:Key="LineSpacingToggleConverter"/>
        <this:PercentageDoubleConverter x:Key="PercentageDoubleConverter" />
        <this:PercentageStringConverter x:Key="PercentageStringConverter" />
        <Style x:Key="StatusBarRibbonButtonStyle" TargetType="{x:Type Syncfusion:RibbonButton}">
            <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
            <Setter Property="CornerRadius" Value="2"/>
            <Setter Property="BorderThickness" Value="1"/>
            <Setter Property="Foreground" Value="#FF00156E"/>
            <Setter Property="Background" Value="Transparent"/>
            <Setter Property="BorderBrush" Value="Transparent"/>
            <Setter Property="Label" Value="New Button"/>
            <Setter Property="LargeIcon">
                <Setter.Value>
                    <BitmapImage UriSource="pack://application:,,,/Syncfusion.Tools.WPF;component/Framework/Ribbon/Resources/WordArt32.png"/>
                </Setter.Value>
            </Setter>
            <Setter Property="SmallIcon">
                <Setter.Value>
                    <BitmapImage UriSource="pack://application:,,,/Syncfusion.Tools.WPF;component/Framework/Ribbon/Resources/WordArt16.png"/>
                </Setter.Value>
            </Setter>
            <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
            <Setter Property="VerticalContentAlignment" Value="Stretch"/>
            <Style.Triggers>
                <Trigger Property="SizeForm" Value="ExtraSmall">
                    <Setter Property="Template">
                        <Setter.Value>
                            <ControlTemplate TargetType="{x:Type Syncfusion:RibbonButton}">
                                <Border x:Name="Bd" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" CornerRadius="{TemplateBinding CornerRadius}" IsEnabled="True" MinHeight="22" Opacity="{TemplateBinding Opacity}">
                                    <Image x:Name="Image" RenderOptions.BitmapScalingMode="NearestNeighbor" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" IsEnabled="True" Height="16" Margin="2" Source="{Binding SmallIcon, Mode=OneWay, RelativeSource={RelativeSource TemplatedParent}}" Stretch="{Binding IconStretch, Mode=OneWay, RelativeSource={RelativeSource TemplatedParent}}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" Width="16"/>
                                </Border>
                                <ControlTemplate.Triggers>
                                    <Trigger Property="IsMouseOver" Value="True">
                                        <Setter Property="Background" TargetName="Bd" Value="#FF3E6DB5"/>
                                    </Trigger>
                                    <Trigger Property="IsPressed" Value="True">
                                        <Setter Property="Background" TargetName="Bd" Value="#FF19478A"/>
                                    </Trigger>
                                    <Trigger Property="IsSelected" Value="True">
                                        <Setter Property="Background" TargetName="Bd" Value="#FF19478A"/>
                                    </Trigger>
                                    <Trigger Property="Syncfusion:SkinStorage.EnableTouch" Value="True">
                                        <Setter Property="Padding" Value="5"/>
                                        <Setter Property="Height" TargetName="Image" Value="20"/>
                                        <Setter Property="Width" TargetName="Image" Value="20"/>
                                        <Setter Property="Width" TargetName="Bd" Value="47"/>
                                        <Setter Property="Height" TargetName="Bd" Value="30"/>
                                    </Trigger>
                                </ControlTemplate.Triggers>
                            </ControlTemplate>
                        </Setter.Value>
                    </Setter>
                </Trigger>
            </Style.Triggers>
        </Style>
    </Syncfusion:RibbonWindow.Resources>

    <Syncfusion:RibbonWindow.StatusBar>
        <Syncfusion:RibbonStatusBar Height="25">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <StackPanel HorizontalAlignment="Left" Orientation="Horizontal" VerticalAlignment="Center">
                    <TextBlock Margin="10,0,0,5" FontSize="12" ToolTip="Page number in document.">
                        <this:RunAdv RunText="Page :" Foreground="{Binding Path=Foreground, Mode=OneWay, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type Syncfusion:RibbonStatusBar}}}"/>
                        <this:RunAdv x:Name="currentPageNumberRun" Foreground="{Binding Path=Foreground, Mode=OneWay, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type Syncfusion:RibbonStatusBar}}}"/>
                        <this:RunAdv RunText="of" Foreground="{Binding Path=Foreground, Mode=OneWay, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type Syncfusion:RibbonStatusBar}}}"/>
                        <this:RunAdv x:Name="pagecountRun" Foreground="{Binding Path=Foreground, Mode=OneWay, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type Syncfusion:RibbonStatusBar}}}"/>
                    </TextBlock>
                </StackPanel>
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" VerticalAlignment="Center" Grid.Column="1">
                    <Syncfusion:RibbonButton Style="{StaticResource StatusBarRibbonButtonStyle}" SizeForm="ExtraSmall" SmallIcon="/Assets/PrintLayout.png" Command="Syncfusion:SfRichTextBoxAdv.LayoutTypeCommand" CommandTarget="{Binding ElementName=richTextBoxAdv}" CommandParameter="Pages" ToolTip="Print Layout"/>
                    <Syncfusion:RibbonButton Style="{StaticResource StatusBarRibbonButtonStyle}" SizeForm="ExtraSmall" SmallIcon="/Assets/WebLayout.png" Command="Syncfusion:SfRichTextBoxAdv.LayoutTypeCommand" CommandTarget="{Binding ElementName=richTextBoxAdv}" CommandParameter="Continuous" ToolTip="Web Layout"/>
                    <TextBlock FontSize="12" VerticalAlignment="Center" Margin="5,0,0,0">
                        <this:RunAdv RunText="{Binding ZoomFactor, Mode=OneWay, Converter={StaticResource PercentageStringConverter}}" Foreground="{Binding Path=Foreground, Mode=OneWay, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type Syncfusion:RibbonStatusBar}}}"/>
                        <this:RunAdv RunText="%" Foreground="{Binding Path=Foreground, Mode=OneWay, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type Syncfusion:RibbonStatusBar}}}"/>
                    </TextBlock>
                    <Slider x:Name="rangeslider" Focusable="False" Value="{Binding ZoomFactor, Mode=TwoWay, Converter={StaticResource PercentageDoubleConverter}}"
                             Foreground="{Binding Path=Foreground, Mode=OneWay, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type Syncfusion:RibbonStatusBar}}}" TickFrequency="0.1" Width="150" TickPlacement="None" Minimum="0.1" Maximum="5" ToolTip="Zoom" />
                </StackPanel>
            </Grid>
        </Syncfusion:RibbonStatusBar>
    </Syncfusion:RibbonWindow.StatusBar>

    <Grid x:Name="Root">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Syncfusion:Ribbon x:Name="ribbon" SnapsToDevicePixels="True" DataContext="{Binding ElementName=richTextBoxAdv}">
            <Syncfusion:Ribbon.QuickAccessToolBar>
                <Syncfusion:QuickAccessToolBar>
                    <Syncfusion:RibbonButton ToolTipService.ShowOnDisabled="True" Label="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=Undo}" Command="Syncfusion:SfRichTextBoxAdv.UndoCommand" SizeForm="ExtraSmall" SmallIcon="/DocumentEditor;component/Images/QAT/Undo16.png" ToolTip="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=UndoToolTip}"/>
                    <Syncfusion:RibbonButton ToolTipService.ShowOnDisabled="True" Label="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=Redo}" Command="Syncfusion:SfRichTextBoxAdv.RedoCommand" SizeForm="ExtraSmall" SmallIcon="/DocumentEditor;component/Images/QAT/Redo16.png" ToolTip="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=RedoToolTip}"/>
                    <Syncfusion:RibbonButton ToolTipService.ShowOnDisabled="True" Label="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=Save}" Command="Syncfusion:SfRichTextBoxAdv.SaveDocumentCommand" SizeForm="ExtraSmall" SmallIcon="/DocumentEditor;component/Images/QAT/Save16.png" ToolTip="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=SaveToolTip}"/>
                </Syncfusion:QuickAccessToolBar>
            </Syncfusion:Ribbon.QuickAccessToolBar>
            <Syncfusion:Ribbon.ApplicationMenu>
                <Syncfusion:ApplicationMenu ApplicationButtonImage="/DocumentEditor;component/Images/Syncfusion.png">
                    <Syncfusion:SimpleMenuButton Label="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=New}" IconSize="25,33" Command="Syncfusion:SfRichTextBoxAdv.NewDocumentCommand" Icon="/DocumentEditor;component/Images/Backstage/New32.png" />
                    <Syncfusion:SimpleMenuButton Label="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=Open}" IconSize="25,33" Command="Syncfusion:SfRichTextBoxAdv.OpenDocumentCommand" Icon="/DocumentEditor;component/Images/Backstage/Open32.png" />
                    <Syncfusion:SimpleMenuButton Label="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=Save}" IconSize="25,33" Command="Syncfusion:SfRichTextBoxAdv.SaveDocumentCommand" Icon="/DocumentEditor;component/Images/Backstage/Save32.png" />
                    <Syncfusion:SplitMenuButton Label="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=SaveAs}" IconSize="25,32" Command="Syncfusion:SfRichTextBoxAdv.SaveAsDocumentCommand" Icon="/DocumentEditor;component/Images/Backstage/SaveAs32.png" >
                        <Syncfusion:ApplicationMenuGroup>
                            <Syncfusion:SimpleMenuButton IconSize="25,25" Label="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=WordDocx}" Description="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=WordDocxToolTip}" Command="Syncfusion:SfRichTextBoxAdv.SaveAsDocumentCommand" CommandParameter=".docx" Icon="/DocumentEditor;component/Images/Backstage/Word2010.png"/>
                            <Syncfusion:SimpleMenuButton IconSize="25,25" Label="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=WordDoc}" Description="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=WordDocToolTip}" Command="Syncfusion:SfRichTextBoxAdv.SaveAsDocumentCommand" CommandParameter=".doc" Icon="/DocumentEditor;component/Images/Backstage/Word2007.png"/>
                            <Syncfusion:SimpleMenuButton IconSize="25,25" Label="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=HtmlDocument}" Description="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=HtmlDocumentToolTip}" Command="Syncfusion:SfRichTextBoxAdv.SaveAsDocumentCommand" CommandParameter=".html" Icon="/DocumentEditor;component/Images/Backstage/HtmlIcon.png"/>
                            <Syncfusion:SimpleMenuButton IconSize="25,25" Label="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=XamlDocument}" Description="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=XamlDocumentToolTip}" Command="Syncfusion:SfRichTextBoxAdv.SaveAsDocumentCommand" CommandParameter=".xaml" Icon="/DocumentEditor;component/Images/Backstage/XamlIcon.png"/>
                            <Syncfusion:SimpleMenuButton IconSize="25,25" Label="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=TextDocument}" Description="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=TextDocumentToolTip}" Command="Syncfusion:SfRichTextBoxAdv.SaveAsDocumentCommand" CommandParameter=".txt" Icon="/DocumentEditor;component/Images/Backstage/TextDocument32.png"/>
                        </Syncfusion:ApplicationMenuGroup>
                    </Syncfusion:SplitMenuButton>
                    <Syncfusion:SimpleMenuButton Label="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=Print}" IconSize="25,33" Command="Syncfusion:SfRichTextBoxAdv.PrintDocumentCommand" Icon="/DocumentEditor;component/Images/Backstage/Print32.png" />
                    <Syncfusion:ApplicationMenu.MenuItems>
                        <TextBlock Text="" Margin="5" Width="210"/>
                    </Syncfusion:ApplicationMenu.MenuItems>
                </Syncfusion:ApplicationMenu>
            </Syncfusion:Ribbon.ApplicationMenu>
            <Syncfusion:Ribbon.BackStage>
                <Syncfusion:Backstage>
                    <Syncfusion:BackstageTabItem Header="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=Info}" >
                        <!--Back Stage tab item content can be of any type.-->
                        <Grid Margin="30">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Grid >
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                <TextBlock Text="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=Information}" FontFamily="Segoe UI" FontSize="20" Margin="10"/>
                                <Rectangle Grid.Row="1" Stroke="Gray" HorizontalAlignment="Stretch" StrokeThickness="1" StrokeDashArray="5"  Margin="10" Height="1"/>
                                <Grid Grid.Row="2" Margin="10">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                    <Syncfusion:DropDownButton Width="87" Height="80" SizeForm="Large" Label="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=ProtectDocument}" LargeIcon="/DocumentEditor;component/Images/Backstage/Protect.png" Margin="5" BorderThickness="1">
                                        <Syncfusion:SimpleMenuButton x:Name="backstageRestrictEditingButton" Icon="/DocumentEditor;component/Images/Home/Editing/RestrictEditing.png" IconSize="30,30" Label="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=RestrictEditing}" Description="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=RestrictEditingToolTip}" />
                                        <Syncfusion:SimpleMenuButton x:Name="backstageEncryptDocumentButton" Icon="/DocumentEditor;component/Images/Backstage/Protect.png" IconSize="30,30" Label="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=EncryptWithPassword}" Description="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=EncryptWithPasswordToolTip}" Command="Syncfusion:SfRichTextBoxAdv.ShowEncryptDocumentDialogCommand"/>
                                    </Syncfusion:DropDownButton>
                                    <StackPanel Grid.Column="1" VerticalAlignment="Top" Margin="3">
                                        <TextBlock Text="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=ProtectDocument}" Grid.Column="1" FontWeight="Bold" Margin="3"/>
                                        <TextBlock Text="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=ProtectDocumentToolTip}" Grid.Column="1" TextWrapping="Wrap" Margin="3"/>
                                    </StackPanel>
                                </Grid>
                                <Rectangle Grid.Row="3" Stroke="Gray" HorizontalAlignment="Stretch" StrokeThickness="1" StrokeDashArray="5"  Margin="10" Height="1"/>
                            </Grid>
                            <Border BorderThickness="0.5" BorderBrush="#FFBCBCBC" Margin="15" Grid.Column="1" HorizontalAlignment="Left" />
                            <Grid  Grid.Column="2">
                                <Button x:Name="rteButton" Margin="20 30" Height="250" Width="390" HorizontalAlignment="Left" VerticalAlignment="Top">
                                    <Button.Style>
                                        <Style TargetType="Button">
                                            <Setter Property="Template">
                                                <Setter.Value>
                                                    <ControlTemplate TargetType="Button">
                                                        <Grid>
                                                            <Border x:Name="Border" Background="Gray" Opacity="0.1">
                                                                <Border.BitmapEffect>
                                                                    <BlurBitmapEffect />
                                                                </Border.BitmapEffect>
                                                            </Border>
                                                            <ContentPresenter Margin="3"/>
                                                        </Grid>
                                                        <ControlTemplate.Triggers>
                                                            <Trigger Property="IsMouseOver" Value="True">
                                                                <Setter Property="Opacity" TargetName="Border" Value="0.5"/>
                                                            </Trigger>
                                                        </ControlTemplate.Triggers>
                                                    </ControlTemplate>
                                                </Setter.Value>
                                            </Setter>
                                        </Style>
                                    </Button.Style>
                                    <Image Source="/DocumentEditor;component/Images/RTE.png" Stretch="Fill"/>
                                </Button>
                            </Grid>
                        </Grid>
                    </Syncfusion:BackstageTabItem>
                    <Syncfusion:BackstageTabItem Header="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=New}">
                        <Grid Margin="30">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Grid >
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                <TextBlock Text="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=AvailableTemplates}" FontFamily="Segoe UI" FontSize="20" Margin="10"/>
                                <Rectangle Grid.Row="1" Stroke="Gray" HorizontalAlignment="Stretch" StrokeThickness="1" StrokeDashArray="5"  Margin="10" Height="1"/>
                                <Grid Grid.Row="2" Margin="10">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                    <Syncfusion:RibbonButton x:Name="backstageNewDocumentButton" SizeForm="Large" LargeIcon="/DocumentEditor;component/Images/Backstage/New32.png" Command="Syncfusion:SfRichTextBoxAdv.NewDocumentCommand" Label="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=BlankDocument}" Width="92" Height="80" Margin="5" BorderBrush="#FFA1A2A4" BorderThickness="1"/>
                                    <StackPanel Grid.Column="1"  VerticalAlignment="Top"  Margin="3">
                                        <TextBlock Text="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=BlankDocument}" Grid.Column="1" FontWeight="Bold" Margin="3" TextWrapping="Wrap"/>
                                        <TextBlock Text="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=BlankDocumentToolTip}" Grid.Column="1" TextWrapping="Wrap" Margin="3"/>
                                    </StackPanel>
                                </Grid>
                                <Rectangle Grid.Row="3" Stroke="Gray" HorizontalAlignment="Stretch" StrokeThickness="1" StrokeDashArray="5"  Margin="10" Height="1"/>
                            </Grid>
                            <Border BorderThickness="0.5" BorderBrush="#FFBCBCBC" Margin="15" Grid.Column="1" HorizontalAlignment="Left" />
                            <Grid Grid.Column="2" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="20,5,0,0" >
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                <TextBlock Text="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=BlankDocument}" FontWeight="Bold" />
                                <Border Grid.Row="1" Margin="0,10,0,0" Height="340" Width="240" BorderThickness="1" BorderBrush="Black" Background="#FFFFFF" />
                                <Syncfusion:RibbonButton x:Name="createDocumentButton" Grid.Row="2" Width="80" Margin="0,10,0,0" Label="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=Create}" Command="Syncfusion:SfRichTextBoxAdv.NewDocumentCommand" SizeForm="Large" BorderBrush="#FFA1A2A4" BorderThickness="1" LargeIcon="/DocumentEditor;component/Images/Backstage/New32.png" HorizontalAlignment="Left"/>
                            </Grid>
                        </Grid>
                    </Syncfusion:BackstageTabItem>
                    <Syncfusion:BackStageCommandButton  x:Name="backstageOpenButton" Header="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=Open}" Command="Syncfusion:SfRichTextBoxAdv.OpenDocumentCommand" />
                    <Syncfusion:BackStageCommandButton x:Name="backstageSaveButton" Header="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=Save}" Command="Syncfusion:SfRichTextBoxAdv.SaveDocumentCommand" />
                    <Syncfusion:BackStageCommandButton x:Name="backstageSaveAsButton" Header="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=SaveAs}" Command="Syncfusion:SfRichTextBoxAdv.SaveAsDocumentCommand" />
                    <Syncfusion:BackStageCommandButton  x:Name="backstagePrintButton" Header="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=Print}" Command="Syncfusion:SfRichTextBoxAdv.PrintDocumentCommand" />
                    <Syncfusion:BackstageTabItem Header="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=Help}">
                        <Grid Margin="30">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Grid >
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                <TextBlock Text="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=UserInterfaceHelp}" FontFamily="Segoe UI" FontSize="20" Margin="10"/>
                                <Rectangle Grid.Row="1" Stroke="Gray" HorizontalAlignment="Stretch" StrokeThickness="1" StrokeDashArray="5"  Margin="10" Height="1"/>
                                <Grid Grid.Row="2" Margin="10">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                    <Syncfusion:RibbonButton x:Name="onlineHelpButton" SizeForm="Large" Width="87" LargeIcon="/DocumentEditor;component/Images/Backstage/Help.png" Height="80" Label="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=OnlineHelp}" Margin="5" BorderBrush="#FFA1A2A4" BorderThickness="1"/>
                                    <StackPanel Grid.Column="1"  VerticalAlignment="Top"  Margin="3">
                                        <TextBlock Text="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=OnlineHelp}" Grid.Column="1" FontWeight="Bold" Margin="3"/>
                                        <TextBlock Text="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=OnlineHelpToolTip}" Grid.Column="1" TextWrapping="Wrap" Margin="3"/>
                                    </StackPanel>
                                </Grid>
                                <Rectangle Grid.Row="3" Stroke="Gray" HorizontalAlignment="Stretch" StrokeThickness="1" StrokeDashArray="5"  Margin="10" Height="1"/>
                                <Grid Grid.Row="4" Margin="10">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                    <Syncfusion:RibbonButton x:Name="gettingStartedButton" SizeForm="Large" Width="87" Height="80" Margin="5" LargeIcon="/DocumentEditor;component/Images/Backstage/Started.png" Label="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=GettingStarted}" BorderBrush="#FFA1A2A4" BorderThickness="1"/>
                                    <StackPanel Grid.Column="1"  VerticalAlignment="Top"  Margin="3">
                                        <TextBlock Text="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=GettingStarted}" Grid.Column="1" FontWeight="Bold" Margin="3"/>
                                        <TextBlock Text="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=GettingStartedToolTip}" Grid.Column="1" TextWrapping="Wrap" Margin="3"/>
                                    </StackPanel>
                                </Grid>
                                <Rectangle Grid.Row="5" Stroke="Gray" HorizontalAlignment="Stretch" StrokeThickness="1" StrokeDashArray="5"  Margin="10" Height="1"/>
                            </Grid>
                            <Border BorderThickness="0.5" BorderBrush="#FFBCBCBC" Margin="15" Grid.Column="1" HorizontalAlignment="Left" />
                        </Grid>
                    </Syncfusion:BackstageTabItem>
                </Syncfusion:Backstage>
            </Syncfusion:Ribbon.BackStage>
            <Syncfusion:RibbonTab Caption="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=Home}">
                <Syncfusion:RibbonBar Header="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=Clipboard}" IsLauncherButtonVisible="False">
                    <Syncfusion:RibbonButton Margin="5" SizeForm="Large" ToolTipService.ShowOnDisabled="True" Label="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=Paste}" IsToggle="False" LargeIcon="/DocumentEditor;component/Images/Home/Clipboard/Paste32.png" Command="Syncfusion:SfRichTextBoxAdv.PasteCommand">
                        <Syncfusion:RibbonButton.ToolTip>
                            <Syncfusion:ScreenTip Description="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=PasteToolTipHeader}" >
                                <StackPanel>
                                    <TextBlock  Text="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=PasteToolTip}" MaxWidth="300" TextWrapping="Wrap"/>
                                </StackPanel>
                            </Syncfusion:ScreenTip>
                        </Syncfusion:RibbonButton.ToolTip>
                    </Syncfusion:RibbonButton>
                    <Syncfusion:RibbonButton Margin="3" SizeForm="Small" ToolTipService.ShowOnDisabled="True" Label="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=Cut}" HorizontalAlignment="Left" IsToggle="False" SmallIcon="/DocumentEditor;component/Images/Home/Clipboard/Cut16.png" Command="Syncfusion:SfRichTextBoxAdv.CutCommand">
                        <Syncfusion:RibbonButton.ToolTip>
                            <Syncfusion:ScreenTip Description="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=CutToolTipHeader}" >
                                <StackPanel>
                                    <TextBlock Text="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=CutToolTip}" MaxWidth="250"  TextWrapping="Wrap" />
                                </StackPanel>
                            </Syncfusion:ScreenTip>
                        </Syncfusion:RibbonButton.ToolTip>
                    </Syncfusion:RibbonButton>
                    <Syncfusion:RibbonButton Margin="3" SizeForm="Small" ToolTipService.ShowOnDisabled="True" Label="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=Copy}" HorizontalAlignment="Left" IsToggle="False" SmallIcon="/DocumentEditor;component/Images/Home/Clipboard/Copy16.png" Command="Syncfusion:SfRichTextBoxAdv.CopyCommand">
                        <Syncfusion:RibbonButton.ToolTip>
                            <Syncfusion:ScreenTip Description="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=CopyToolTipHeader}">
                                <StackPanel>
                                    <TextBlock Text="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=CutToolTip}" MaxWidth="250" TextWrapping="Wrap"/>
                                </StackPanel>
                            </Syncfusion:ScreenTip>
                        </Syncfusion:RibbonButton.ToolTip>
                    </Syncfusion:RibbonButton>
                </Syncfusion:RibbonBar>
                <Syncfusion:RibbonBar Header="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=Font}" IsLargeButtonPanel="False" LauncherCommand="Syncfusion:SfRichTextBoxAdv.ShowFontDialogCommand">
                    <Syncfusion:ButtonPanel Height="24" SeparatorVisibility="Collapsed">
                        <Syncfusion:RibbonComboBox x:Name="fontFamilyComboBox" Width="110" IsEditable="True" ToolTipService.ShowOnDisabled="True" Text="{Binding Selection.CharacterFormat.FontFamily, Mode=OneWay, Converter={StaticResource FontFamilyStringConverter}}" SelectedValue="{Binding Selection.CharacterFormat.FontFamily, Mode=TwoWay, Converter={StaticResource FontFamilyStringConverter}}">
                            <Syncfusion:RibbonComboBox.ToolTip>
                                <Syncfusion:ScreenTip Description="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=FontToolTipHeader}" >
                                    <TextBlock Text="{Binding Source={StaticResource DocumentEditorResourceWrapper}, Path=FontToolTip}" MaxWidth="250" TextWrapping="Wrap"/>
